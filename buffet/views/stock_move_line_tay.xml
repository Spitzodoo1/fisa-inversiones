<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="tray_stock_move_line_inherited" model="ir.ui.view">
        <field name="name">stock.move.line.form.inherited</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_move_line_form"/>
        <field name="arch" type="xml">
            <field name="location_id" position="after">
                <field name="tray_id"/>
            </field>
        </field>
    </record>
    <record id="tray_for_stock_move_inherited" model="ir.ui.view">
        <field name="name">stock.move.form.inherited</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='main_grp_col2']" position="inside">

                <field name="tray_id"/>
            </xpath>
        </field>
    </record>
</odoo>