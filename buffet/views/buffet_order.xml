<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="buffet_line_action" model="ir.actions.act_window">
        <field name="name">Buffet Order</field>
        <field name="res_model">buffet.line</field>
        <field name="view_mode">kanban,tree,form</field>
    </record>
    <record id="buffet_order_form" model="ir.ui.view">
        <field name="name">buffet.buffet.line.form</field>
        <field name="model">buffet.line</field>
        <field name="arch" type="xml">
            <form string="Test">
                <sheet>
                    <group>
                        <group>
                       <field name="product_id"/>
                            <field name="quantity"/>
                            <field name="exp_hour"/>
                            <field name="create_date"/>
                            <field name="apply"/>
                            <field name="available"/>
                        </group></group>

                </sheet>
            </form>
        </field>
    </record>


</odoo>